{
  "name": "c2farms-backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node --watch src/server.js",
    "start": "node src/server.js",
    "db:migrate": "npx prisma migrate dev --schema=src/prisma/schema.prisma",
    "db:push": "npx prisma db push --schema=src/prisma/schema.prisma",
    "db:seed": "node src/prisma/seed.js",
    "db:studio": "npx prisma studio --schema=src/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "pdfmake": "^0.2.12",
    "socket.io": "^4.8.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "prisma": "^5.22.0"
  }
}
